
&НаСервере
Процедура ПриСозданииНаСервере(Отказ, СтандартнаяОбработка)
	Элементы.ТипКода.СписокВыбора.Добавить(16,"QR");
	Элементы.ТипКода.СписокВыбора.Добавить(4,"CODE128");
	Элементы.ТипКода.СписокВыбора.Добавить(14,"EAN13Addon2");
    Элементы.ТипКода.СписокВыбора.Добавить(15,"EAN13Addon5");
КонецПроцедуры

&НаКлиенте
Процедура ВыполнитьКоманду(Команда)
	ТабДок=ВыполнитьНаСервере();
	ТабДок.Показать();
КонецПроцедуры

&НаСервере
Функция  ВыполнитьНаСервере()
	ПараметрыПечати = новый Структура;
	ПараметрыПечати.Вставить("Ширина",?(ЗначениеЗаполнено(ШиринаШтрихКода),ШиринаШтрихКода,10));
	ПараметрыПечати.Вставить("Высота",?(ЗначениеЗаполнено(ВысотаШтрихКода),ВысотаШтрихКода,15));
	ПараметрыПечати.Вставить("ТипКода",Объект.ТипКода);
	Массив = новый Массив;
	Массив.Добавить(Объект.ПолеВводаШтрихКода);
	Данные=РеквизитФормыВЗначение("Объект");
	ТабДок=Данные.Печать(ПараметрыПечати,Массив);
	Возврат ТабДок;
	КонецФункции