		<field xsi:type="DataSetFieldField">
			<dataPath>РезультатУпр</dataPath>
			<field>РезультатУпр</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Результат упр</v8:content>
				</v8:item>
