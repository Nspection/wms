#Область СозданиеЯчеек
&НаСервере
Процедура СоздатьНаСервере()
	РодительСкладскогоПомещения=НайтиСоздатьРодительСкладскогоПомещения();
	СтруктураПараметров=новый Структура;
	СтруктураПараметров.Вставить("Линия",0);
	СтруктураПараметров.Вставить("Стеллаж",0);
	СтруктураПараметров.Вставить("Ярус",0);
	Для n=Объект.ЛинияС по Объект.ЛинияПо Цикл 
		РодительЛинии=НайтиСоздатьРодительЛинии(n,РодительСкладскогоПомещения);
		СтруктураПараметров.Линия=n;
		СоздатьНаСервереСтелаж(РодительЛинии,СтруктураПараметров);
	КонецЦикла;
КонецПроцедуры
&НаСервере
Процедура СоздатьНаСервереСтелаж(РодительЛинии,СтруктураПараметров)
	Для n=Объект.СтеллажС по Объект.СтеллажПО Цикл 
		РодительСтеллажа=НайтиСоздатьРодительСтеллажа(n,РодительЛинии);
		СтруктураПараметров.Стеллаж=n;
		СоздатьНаСервереЯрус(РодительСтеллажа,СтруктураПараметров);
	КонецЦикла;
КонецПроцедуры
&НаСервере
Процедура СоздатьНаСервереЯрус(РодительСтелажа,СтруктураПараметров)	
	Для n=Объект.ЯрусС по Объект.ЯрусПО Цикл 
		СтруктураПараметров.Ярус=n;	
		ПоискСозданияЭлемента(СтруктураПараметров,РодительСтелажа)
	КонецЦикла;
КонецПроцедуры
&НаКлиенте
Процедура Создать(Команда)
	Если Объект.ЛинияС=0 Тогда 
		Возврат
	КонецЕсли;	
	Если Объект.ЛинияПо=0 Тогда 
		Возврат
	КонецЕсли;	
	Если Объект.СтеллажС=0 Тогда 
		Возврат
	КонецЕсли;	
	Если Объект.СтеллажПО=0 Тогда 
		Возврат
	КонецЕсли;
	Если Объект.ЯрусС=0 Тогда 
		Возврат
	КонецЕсли;
	Если Объект.ЯрусПО=0 Тогда 
		Возврат
	КонецЕсли;
	Если Объект.СкладскоеПомещение.Пустая() Тогда 
		Возврат
	КонецЕсли;	
	Оповещение=новый ОписаниеОповещения("СоздатьОповещение",ЭтаФорма);
	ПоказатьВопрос(Оповещение,"Вы уверены, что хотите создать новые ячейки?",РежимДиалогаВопрос.ДаНет);
КонецПроцедуры
&НаКлиенте
Процедура СоздатьОповещение(Результат,Параметры) Экспорт 
	Если Результат=КодВозвратаДиалога.Нет Тогда 
		Возврат
	КонецЕсли;	
	СоздатьНаСервере();
КонецПроцедуры
&НаСервере
Функция НайтиСоздатьРодительСкладскогоПомещения()
	НаименованиеРодителя="Зона "+Объект.СкладскоеПомещение.Префикс;
	Возврат  ПоискСозданиеРодителя(НаименованиеРодителя);
КонецФункции
&НаСервере
Функция НайтиСоздатьРодительЛинии(НомерЛинии,Родитель)
	НаименованиеРодителя=Объект.СкладскоеПомещение.Префикс+"-"+ДобавитьНули(НомерЛинии);
	Возврат  ПоискСозданиеРодителя(НаименованиеРодителя,Родитель);
КонецФункции
&НаСервере
Функция НайтиСоздатьРодительСтеллажа(НомерСтеллажа,Родитель)
	НаименованиеРодителя=Родитель.Наименование+"-"+ДобавитьНули(НомерСтеллажа);
	Возврат  ПоискСозданиеРодителя(НаименованиеРодителя,Родитель);
КонецФункции
&НаСервере
Функция ПоискСозданиеРодителя(НаименованиеРодителя,Родитель=Неопределено)
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	итСкладскиеЯчейки.Ссылка
	|ИЗ
	|	Справочник.итСкладскиеЯчейки КАК итСкладскиеЯчейки
	|ГДЕ
	|	итСкладскиеЯчейки.Наименование = &Наименование
	|	И итСкладскиеЯчейки.СкладскоеПомещение = &СкладскоеПомещение
	|	И ВЫБОР
	|			КОГДА &Родитель = НЕОПРЕДЕЛЕНО
	|				ТОГДА ИСТИНА
	|			ИНАЧЕ итСкладскиеЯчейки.Родитель = &Родитель
	|		КОНЕЦ";
	
	Запрос.УстановитьПараметр("Наименование", НаименованиеРодителя);
	Запрос.УстановитьПараметр("СкладскоеПомещение", Объект.СкладскоеПомещение);
	Запрос.УстановитьПараметр("Родитель", Родитель);
	РезультатЗапроса = Запрос.Выполнить();
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	
	Пока ВыборкаДетальныеЗаписи.Следующий() Цикл
		Возврат ВыборкаДетальныеЗаписи.Ссылка;
	КонецЦикла;
	НовыйГрупповойЭлемент=Справочники.итСкладскиеЯчейки.СоздатьГруппу();
	НовыйГрупповойЭлемент.Наименование=НаименованиеРодителя;
	Если Родитель<>Неопределено  Тогда 
		НовыйГрупповойЭлемент.Родитель=Родитель;
	КонецЕсли;
	НовыйГрупповойЭлемент.СкладскоеПомещение=Объект.СкладскоеПомещение;
	НовыйГрупповойЭлемент.Записать();
	Возврат НовыйГрупповойЭлемент.Ссылка;
	
КонецФункции
&НаСервере
Процедура ПоискСозданияЭлемента(СтруктураПараметров,Родитель)
	Линия=ДобавитьНули(СтруктураПараметров.Линия);
	Стеллаж=ДобавитьНули(СтруктураПараметров.Стеллаж);
	Ярус=ДобавитьНули(СтруктураПараметров.Ярус,1);	
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	итСкладскиеЯчейки.Ссылка
	|ИЗ
	|	Справочник.итСкладскиеЯчейки КАК итСкладскиеЯчейки
	|ГДЕ
	|	итСкладскиеЯчейки.СкладскоеПомещение = &СкладскоеПомещение
	|	И итСкладскиеЯчейки.Линия = &Линия
	|	И итСкладскиеЯчейки.Стеллаж = &Стеллаж
	|	И итСкладскиеЯчейки.Ярус = &Ярус";
	
	Запрос.УстановитьПараметр("Линия", Линия);
	Запрос.УстановитьПараметр("СкладскоеПомещение", Объект.СкладскоеПомещение);
	Запрос.УстановитьПараметр("Стеллаж", Стеллаж);
	Запрос.УстановитьПараметр("Ярус", Ярус);
	
	РезультатЗапроса = Запрос.Выполнить();
	СсылкаНаЭлемент=Неопределено;
	Если  РезультатЗапроса.Пустой() Тогда 
		НовыйЭлемент=Справочники.итСкладскиеЯчейки.СоздатьЭлемент();
		НовыйЭлемент.Наименование=Объект.СкладскоеПомещение.Префикс+"-"+Линия+"-"+Стеллаж+"-"+Ярус;
		НовыйЭлемент.Линия=Линия;
		НовыйЭлемент.Стеллаж=Стеллаж;
		НовыйЭлемент.Ярус=ярус;
		НовыйЭлемент.Родитель=Родитель;
		НовыйЭлемент.СкладскоеПомещение=Объект.СкладскоеПомещение;
		НовыйЭлемент.Зона=Объект.Зона;
		НовыйЭлемент.ВидСкладскойДеятельности=Объект.ВидСкладскойДеятельности;
		НовыйЭлемент.Записать();
		СсылкаНаЭлемент=НовыйЭлемент.Ссылка;
	иначе
		Выборка=РезультатЗапроса.Выбрать();
		Если Выборка.Следующий() Тогда 
			СсылкаНаЭлемент=Выборка.Ссылка;
		КонецЕсли;
	КонецЕсли;	
	Если Объект.ФормироватьШК Тогда 
		НаборЗаписей=РегистрыСведений.ИтWMSШтрихКоды.СоздатьНаборЗаписей();
		НаборЗаписей.Отбор.Объект.Установить(СсылкаНаЭлемент);
		НаборЗаписей.Прочитать();
		НаборЗаписей.Очистить();
		НоваяЗапись=НаборЗаписей.Добавить();
		НоваяЗапись.Объект=СсылкаНаЭлемент;
		НоваяЗапись.ШтрихКод=СсылкаНаЭлемент.Наименование;
		НаборЗаписей.Записать();
	КонецЕсли;
	
	
КонецПроцедуры
#КонецОбласти

#Область УстановкаСнятиеБлокировки

&НаСервере	
Процедура ОбходЭлементовПоТаблице(ПараметрыФункций)
	ПараметрыФункций.Вставить("Линия",0);
	ПараметрыФункций.Вставить("Стеллаж",0);
	ПараметрыФункций.Вставить("Ярус",0);
	Для Каждого стр из Объект.ЯчейкиКОбработке Цикл
		ОбходПоЛиниям(ПараметрыФункций,стр);	
	КонецЦикла;
КонецПроцедуры
&НаСервере	
Процедура ОбходПоЛиниям(ПараметрыФункций,стр)
	Для n=стр.ЛинияС по стр.ЛинияПо Цикл 
		ПараметрыФункций.Линия=n;
		ОбходПоСтелажам(ПараметрыФункций,стр);
	КонецЦикла;
КонецПроцедуры
&НаСервере	
Процедура ОбходПоСтелажам(ПараметрыФункций,стр)
	Для n=стр.СтеллажС по стр.СтеллажПО Цикл 
		ПараметрыФункций.Стеллаж=n;
		ОбходПоЯрусам(ПараметрыФункций,стр);
	КонецЦикла;
КонецПроцедуры
&НаСервере	
Процедура ОбходПоЯрусам(ПараметрыФункций,стр)
	Для n=стр.ЯрусС по стр.ЯрусПО Цикл 
		ПараметрыФункций.Ярус=n;	
		ИзменениеЭлементов(ПараметрыФункций,стр)
	КонецЦикла;
КонецПроцедуры
&НаСервере	
Процедура ИзменениеЭлементов(ПараметрыФункций,стр)
	Линия=ДобавитьНули(ПараметрыФункций.Линия);
	Стеллаж=ДобавитьНули(ПараметрыФункций.Стеллаж);
	Ярус=ДобавитьНули(ПараметрыФункций.Ярус,1);	
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	итСкладскиеЯчейки.Ссылка
	|ИЗ
	|	Справочник.итСкладскиеЯчейки КАК итСкладскиеЯчейки
	|ГДЕ
	|	итСкладскиеЯчейки.СкладскоеПомещение = &СкладскоеПомещение
	|	И итСкладскиеЯчейки.Линия = &Линия
	|	И итСкладскиеЯчейки.Стеллаж = &Стеллаж
	|	И итСкладскиеЯчейки.Ярус = &Ярус";
	
	Запрос.УстановитьПараметр("Линия", Линия);
	Запрос.УстановитьПараметр("СкладскоеПомещение", стр.СкладскоеПомещение);
	Запрос.УстановитьПараметр("Стеллаж", Стеллаж);
	Запрос.УстановитьПараметр("Ярус", Ярус);
	
	РезультатЗапроса=Запрос.Выполнить();
	
	Выборка=РезультатЗапроса.Выбрать();
	
	Пока Выборка.Следующий() Цикл 
		ОбъектКИзменению=Выборка.Ссылка.ПолучитьОбъект();
		Если ПараметрыФункций.Свойство("Блокировка") Тогда 
			Если ПараметрыФункций.Блокировка Тогда
				ОбъектКИзменению.Заблокирована=Истина;
			иначе
				ОбъектКИзменению.Заблокирована=Ложь;
			КонецЕсли;
		КонецЕсли;	
		Если ПараметрыФункций.Свойство("УстановкаРядаПролета") Тогда 
			Если ПараметрыФункций.УстановкаРядаПролета Тогда
				ОбъектКИзменению.НомерРяда_Пролета=НомерРяда;
			КонецЕсли;
		КонецЕсли;	
 
		Если Комментарий="*" Тогда 
			ОбъектКИзменению.Комментарий="";
		ИначеЕсли Комментарий="-" Тогда
			ОбъектКИзменению.Комментарий=ОбъектКИзменению.Комментарий;
		Иначе 
			ОбъектКИзменению.Комментарий=Комментарий;
		КонецЕсли;
		ОбъектКИзменению.Записать();
	КонецЦикла;
	
КонецПроцедуры
